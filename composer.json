{
    "name": "dwwe/whoops",
    "license": "MIT",
    "description": "considerable php error and exception handling",
    "keywords": ["library", "error", "handling", "exception", "whoops", "throwable"],
    "homepage": "https://github.com/dwwe2017/whoops",
    "authors": [
        {
            "name": "Daniel W.",
            "homepage": "https://www.dwwe.de",
            "role": "Developer"
        },
        {
            "name": "Filipe Dobreira",
            "homepage": "https://github.com/filp",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^7.3",
        "psr/log": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7",
        "mockery/mockery": "^0.9 || ^1.0",
        "symfony/var-dumper": "^2.6 || ^3.0 || ^4.0"
    },
    "suggest": {
        "symfony/var-dumper": "Pretty print complex values better with var-dumper available",
        "whoops/soap": "Formats errors as SOAP responses"
    },
    "autoload": {
        "psr-4": {
            "Whoops\\": "src/Whoops/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Whoops\\": "tests/Whoops/"
        }
    }
}
